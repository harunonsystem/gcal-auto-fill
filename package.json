{
  "name": "gcal-auto-fill",
  "description": "Browser extension that assists with Google Calendar event creation",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "tsc --noEmit",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "test": "vitest",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "oxlint": "^1.34.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "chrono-node": "^2.9.0"
  },
  "packageManager": "bun@1.3.5"
}
